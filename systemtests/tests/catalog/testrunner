#!/bin/sh
#
# Run a simple backup
#   then restore it.
#
TestName="$(basename "$(pwd)")"
export TestName

. ./environment
. ${scripts}/functions

${scripts}/cleanup
${scripts}/setup

exit_failure()
{
  estat=1
  end_test
}

start_test

./../../../../b/core/src/tests/catalog || exit_failure

end_test
